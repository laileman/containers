$ORIGIN nginx.local.
$TTL 3600

@       IN  SOA     ns.nginx.local. admin.nginx.local. (
                        2023082001  ; 序列号（建议用日期格式）
                        86400       ; 刷新时间 (1天)
                        7200        ; 重试时间 (2小时)
                        604800      ; 过期时间 (7天)
                        3600 )      ; 否定缓存 TTL (1小时)

@       IN  NS      ns.nginx.local.
ns      IN  A       192.168.100.205
@       IN  A       192.168.100.205
*       IN  A       192.168.100.205
